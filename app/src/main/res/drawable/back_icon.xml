<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <stroke
                android:width="0dp"
                android:color="#ffffff"/>
            <size
                android:width="35dp"
                android:height="30dp"/>

        </shape>
    </item>

    <item
        android:top="-15dp"
        android:left="9dp"
        android:right="5dp" >
        <shape android:shape="line">

            <stroke
                android:width="1dp"
                android:color="#ffffff"/>

        </shape>
    </item>

    <item
        android:top="15dp"
        android:left="9dp"
        android:right="5dp" >
        <shape android:shape="line">

            <stroke
                android:width="1dp"
                android:color="#ffffff"/>

        </shape>
    </item>

    <item
        android:top="7dp"
        android:left="25dp" >

        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>

    <item
        android:bottom="7dp"
        android:left="25dp" >

        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>

    <item
        android:bottom="-7dp"
        android:right="23dp" >

        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>

    <item
        android:bottom="7dp"
        android:right="23dp" >

        <rotate
            android:fromDegrees="135"
            android:toDegrees="135">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>

    <item

        android:left="15dp"
        android:right="8dp">

        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>

    <item

        android:left="15dp"
        android:right="8dp">

        <rotate
            android:fromDegrees="135"
            android:toDegrees="135">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="#ffffff"/>

            </shape>
        </rotate>
    </item>
</layer-list>
